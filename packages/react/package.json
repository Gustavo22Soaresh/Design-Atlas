{
  "name": "@design-atlas/react",
  "version": "0.1.1",
  "description": "React components for Design Atlas design system",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap",
    "clean": "rimraf dist",
    "prepack": "pnpm build"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^5.0.0",
    "tsup": "^8.0.0",
    "rimraf": "^5.0.0"
  },
  "keywords": [
    "react",
    "components",
    "design-system",
    "ui"
  ],
  "author": "Design Atlas Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OSMBrasil/design-atlas.git",
    "directory": "packages/react"
  }
}
